generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guild {
  id     String  @id
  prefix String?
  vip    String?

  @@map("guild")
}

model Status {
  id   Int    @id @default(autoincrement())
  name String
  type Int    @default(0)

  @@map("status")
}

model Tag {
  id    String @id @db.Citext
  user  String
  src   String
  count Int    @default(0)

  @@map("tag")
}
